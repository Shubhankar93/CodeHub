package miscellaneous;

public class FibanacciSeries {

	public static void main(String[] args) {
		int a= 1;
		int b= 2;
		//int c = a+b;
		System.out.println(a);
		System.out.println(b);
		//System.out.println(c);
		int count = 0;
		while (count<=5)
		{
		int c = a+b;
		 a = b;
		 b = c;
		 System.out.println(c);
		/*if (c<=50){
		System.out.println(c);
		}else{
			break;
		}*/


		}


	}

}
